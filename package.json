{
  "name": "textactor-explorer",
  "version": "0.1.0",
  "description": "A nodejs module for exploring TextActors(Named entities) in texts.",
  "main": "./lib/api/index.js",
  "types": "./types/api/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textactor/textactor-explorer.git"
  },
  "keywords": [
    "textactor",
    "textactor-explorer"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textactor/textactor-explorer/issues"
  },
  "homepage": "https://github.com/textactor/textactor-explorer#readme",
  "devDependencies": {
    "@types/boom": "^7.2.0",
    "@types/mongoose": "^5.0.18",
    "@types/ms": "^0.7.30",
    "@types/node": "latest",
    "@types/shortid": "^0.0.29",
    "ava": "^0.23.0",
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "dependencies": {
    "@textactor/known-names": "^0.1.0",
    "atonic": "^1.0.4",
    "boom": "^7.2.0",
    "concepts-parser": "^1.3.1",
    "entity-finder": "^0.5.2",
    "mongoose": "^5.2.0",
    "partial-name": "^0.1.4",
    "root-name": "^0.1.8",
    "shortid": "^2.2.8",
    "standard-text": "^0.1.1",
    "text-country": "^0.1.1",
    "wiki-entity": "^0.4.0"
  }
}
